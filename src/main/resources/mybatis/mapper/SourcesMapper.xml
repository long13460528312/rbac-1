<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.offcn.dao.SourcesDao">

    <select id="selectById" resultType="com.offcn.bean.SourcesBean">
            select * from sources where id=#{0}
    </select>

    <select id="findByPid" resultType="com.offcn.bean.SourcesBean">
        select * from sources where pid=#{0}
    </select>

</mapper>